t = user.entries + 1;\n\n    const text = { input: entry.text };\n    \n    // update number of entries completed\n    dispatch({ type: 'COMPLETE_ENTRY', user: {\n      entries: user.entries + 1,\n      text,\n    }});\n\n    setEntry({\n      ...entry,\n      completed: true\n    });\n    \n    // update database info\n    updateEntries(count);\n\n    openModal();\n  }\n\n  return (\n    <div className=\"entry-container\">\n      <div className=\"btns-container\">\n        <button className=\"dest-btns dashboard-route-btn\" onClick={() => setDest('dashboard')}>Return to Dashboard</button>\n      </div>\n\n      <div className=\"form-container\">\n        <form onSubmit={handleSubmit} className=\"entry-form\">\n          <Entry entry={entry} />\n          <textarea \n            value={entry.text}\n            id=\"entry-input\"\n            onChange={handleChange}\n            rows={10}\n            cols={50}\n          />\n          <button className=\"submit-btn toggle-button\">Complete Entry</button>\n        </form>\n\n        <Modal modalState={modalState} closeModal={closeModal} />\n      </div>\n    </div>\n  );\n}\n\nexport default EntryForm;\n","/home/bzweb/projects/custom/mindful/frontend/src/components/Entry.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/modules/entries.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/Modal.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/LogoutButton.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/RecVideos.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/Timer.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/RecApps.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/Info.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/Login.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/CreateUser.js",[],"/home/bzweb/projects/custom/mindful/frontend/src/components/Settings.j